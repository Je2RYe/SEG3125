<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>First Name</h2>
    <ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].first_name %> :  <%= results[0][i].count %></li>
    <% } %>    
    </ul>
    <h2>Last Name</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].last_name %> :  <%= results[1][i].count %></li>
    <% } %>    
    </ul>
    <h2>Grade</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].singleChoice1 %> :  <%= results[2][i].count %></li>
    <% } %>    
    </ul>
    <h2>Often used Feature</h2>
    <ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].select1 %> :  <%= results[3][i].count %></li>
    <% } %>    
    </ul>
    <h2>Posibility to recommend the website</h2>
    <ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].singleChoice2 %> :  <%= results[4][i].count %></li>
    <% } %>    
    </ul>
    <h2>Overall User experience</h2>
    <ul>
    <% for (var i=0; i<results[5].length; i++){ %>
        <li><%= results[5][i].singleChoice3 %> :  <%= results[5][i].count %></li>
    <% } %>    
    </ul>
    <h2>Role of users in university</h2>
    <ul>
    <% for (var i=0; i<results[6].length; i++){ %>
        <li><%= results[6][i].singleChoice4 %> :  <%= results[6][i].count %></li>
    <% } %>    
    </ul>
    <h2>Most used part of the website</h2>
    <ul>
    <% for (var i=0; i<results[7].length; i++){ %>
        <li><%= results[7][i].select2 %> :  <%= results[7][i].count %></li>
    <% } %>    
    </ul>
    <h2>Comments</h2>
    <ul>
    <% for (var i=0; i<results[8].length; i++){ %>
        <li><%= results[8][i].comments %> :  <%= results[8][i].count %></li>
    <% } %>    
    </ul>
</body>
</html>